{
    "ports": [
        "ffmpeg@3.4.13"
    ],
    "override_ports": {
        "ffmpeg@3.4.13": {
            "arguments": [
                "--sysroot=${SYSROOT}",
                "--cross-prefix=${CROSS_PREFIX}",
                "--arch=${SYSTEM_PROCESSOR}",
                "--target-os=${SYSTEM_NAME}",
                "--enable-cross-compile",
                "--disable-programs",
                "--disable-doc",
                "--disable-libx264",
                "--disable-libx265",
                "--enable-pic",
                "--enable-gpl",
                "--pkg-config=pkg-config"
            ],
            "dependencies": [ ]
        }
    }
}