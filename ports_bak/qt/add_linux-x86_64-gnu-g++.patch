diff -uNr "src/qtbase/mkspecs/linux-x86_64-gnu-g++/ qmake.conf" "src_fix/qtbase/mkspecs/linux-x86_64-gnu-g++/ qmake.conf"
--- "src/qtbase/mkspecs/linux-x86_64-gnu-g++/ qmake.conf"	1970-01-01 08:00:00.000000000 +0800
+++ "src_fix/qtbase/mkspecs/linux-x86_64-gnu-g++/ qmake.conf"	2025-02-25 21:16:24.077321780 +0800
@@ -0,0 +1,24 @@
+#
+# qmake configuration for building with x86_64-linux-gnu-g++
+#
+
+MAKEFILE_GENERATOR      = UNIX
+CONFIG                 += incremental
+QMAKE_INCREMENTAL_STYLE = sublib
+
+include(../common/linux.conf)
+include(../common/gcc-base-unix.conf)
+include(../common/g++-unix.conf)
+
+# modifications to g++.conf
+QMAKE_CC                = x86_64-linux-gnu-gcc
+QMAKE_CXX               = x86_64-linux-gnu-g++
+QMAKE_LINK              = x86_64-linux-gnu-g++
+QMAKE_LINK_SHLIB        = x86_64-linux-gnu-g++
+
+# modifications to linux.conf
+QMAKE_AR                = x86_64-linux-gnu-ar cqs
+QMAKE_OBJCOPY           = x86_64-linux-gnu-objcopy
+QMAKE_NM                = x86_64-linux-gnu-nm -P
+QMAKE_STRIP             = x86_64-linux-gnu-strip
+load(qt_config)
\ No newline at end of file
