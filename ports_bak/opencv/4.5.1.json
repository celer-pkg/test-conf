{
    "url": "https://gitee.com/baolingyi/opencv.git",
    "ref": "4.5.1",
    "build_configs": [
        {
            "pattern": "*linux*",
            "patches": [
                "4.5.1_fix_github_download.patch"
            ],
            "build_system": "cmake",
            "library_type": "shared",
            "options": [
                "-DENABLE_NEON=OFF",
                "-DBUILD_TESTING=OFF",
                "-DBUILD_DOCS=OFF",
                "-DBUILD_ANDROID_EXAMPLES=OFF",
                "-DBUILD_EXAMPLES=OFF",
                "-DBUILD_PERF_TESTS=OFF",
                "-DBUILD_TESTS=OFF",
                "-DWITH_FFMPEG=OFF",
                "-DWITH_GTK=OFF",
                "-DWITH_GSTREAMER=OFF"
            ],
            "dependencies": [
                "zlib@v1.3.1"
            ]
        }
    ]
}