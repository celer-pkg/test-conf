    "ref": "1.8.11",
    "build_configs": [
        {
            "pattern": "*linux*",
            "build_system": "makefiles",
            "library_type": "shared",
            "build_tools": [
                "xutils-dev"
            ],
            "env_vars": [],
            "options": [
                "--host=${HOST}",
                "--with-keysymdefdir=${INSTALLED_DIR}/include/X11"
            ],
            "dependencies": [
                "libxtrans@xtrans-1.5.2",
                "xorgproto@2024.1",
                "libxcb@1.17.0",
                "libxau@1.0.12"
            ]
        }
    ]
}