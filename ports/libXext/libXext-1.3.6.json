{
    "url": "https://gitlab.freedesktop.org/xorg/lib/libxext.git",
    "name": "libXext",
    "version": "libXext-1.3.6",
    "build_configs": [
        {
            "pattern": "aarch64-linux*",
            "build_tool": "makefiles",
            "library_type": "shared",
            "env_vars": [
                "NOCONFIGURE=1"
            ],
            "arguments": [
                "--host=${HOST}",
                "--cache-file=aarch64-linux.cache"
            ],
            "dependencies": []
        },
        {
            "pattern": "x86_64-linux*",
            "build_tool": "makefiles",
            "library_type": "shared",
            "env_vars": [
                "NOCONFIGURE=1"
            ],
            "arguments": [
                "--host=${HOST}"
            ],
            "dependencies": []
        }
    ]
}