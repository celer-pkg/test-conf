{
    "url": "https://gitlab.freedesktop.org/xorg/lib/libxext.git",
    "name": "libXext",
    "version": "libXext-1.3.6",
    "build_configs": [
        {
            "pattern": "*linux*",
            "build_tool": "makefiles",
            "library_type": "shared",
            "env_vars": [
                "NOCONFIGURE=1"
            ],
            "arguments": [
                "--host=arm-linux",
                "--cache-file=arm-linux.cache"
            ],
            "dependencies": []
        }
    ]
}