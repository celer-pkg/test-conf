{
    "url": "https://gitee.com/phil-zhang/util-linux.git",
    "name": "util-linux",
    "version": "v2.40.4",
    "build_configs": [
        {
            "pattern": "*linux*",
            "build_tool": "makefiles",
            "install_with_sudo": true,
            "env_vars": [],
            "arguments": [
                "--host=${HOST}",
                "--disable-use-tty-group",
                "--disable-makeinstall-chown"
            ],
            "dependencies": [
                "sqlite3@version-3.47.0"
            ]
        }
    ]
}