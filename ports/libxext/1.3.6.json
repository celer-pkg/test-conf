{
    "url": "https://gitlab.freedesktop.org/xorg/lib/libxext.git",
    "name": "libxext",
    "ref": "libXext-1.3.6",
    "build_configs": [
        {
            "pattern": "*linux*",
            "system_tools": [],
            "patches": [],
            "build_tool": "makefiles",
            "library_type": "shared",
            "env_vars": [
                "NOCONFIGURE=1",
                "xorg_cv_malloc0_returns_null=yes"
            ],
            "options": [
                "--host=${HOST}"
            ],
            "dependencies": [
                "libice@1.1.2"
            ]
        }
    ]
}