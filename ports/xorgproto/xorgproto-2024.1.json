{
    "url": "https://gitlab.freedesktop.org/xorg/proto/xorgproto.git",
    "name": "xorgproto",
    "version": "xorgproto-2024.1",
    "build_configs": [
        {
            "pattern": "*linux*",
            "build_tool": "makefiles",
            "library_type": "shared",
            "system_tools": [
                "autoconf"
            ],
            "env_vars": [
                "NOCONFIGURE=1"
            ],
            "arguments": [
                "--host=${HOST}",
                "--without-fop",
                "--enable-option-checking"
            ],
            "dependencies": [
                "util-macros@1.20.2",
                "xorg-sgml-doctools@xorg-sgml-doctools-1.12.1"
            ],
            "dev_dependencies": [
                "libxslt@v1.1.42"
            ]
        }
    ]
}