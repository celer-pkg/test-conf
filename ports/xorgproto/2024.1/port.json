{
    "url": "https://gitlab.freedesktop.org/xorg/proto/xorgproto.git",
    "ref": "xorgproto-2024.1",
    "build_configs": [
        {
            "pattern": "*",
            "build_system": "makefiles",
            "library_type": "shared",
            "build_tools": [
                "autoconf",
                "xutils-dev",
                "msys2:xorg-util-macros"
            ],
            "options": [
                "--host=${HOST}",
                "--without-fop",
                "--enable-option-checking"
            ],
            "dependencies": [
                "xorg-sgml-doctools@1.12.1"
            ]
        }
    ]
}