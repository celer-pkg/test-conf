{
    "url": "https://gitee.com/phil-zhang/glib.git",
    "name": "glib",
    "version": "2.62.0",
    "build_configs": [
        {
            "pattern": "*linux*",
            "build_tool": "meson",
            "library_type": "shared",
            "env_vars": [ ],
            "arguments": [
                "-Dlibmount=false",
                "--libdir=lib"
            ],
            "dependencies": [
                "zlib@v1.3.1",
                "pcre2@pcre2-10.44",
                "libffi@v3.4.6"
            ]
        }
    ]
}