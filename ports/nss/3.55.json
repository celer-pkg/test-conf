{
    "url": "https://archive.mozilla.org/pub/security/nss/releases/NSS_3_55_RTM/src/nss-3.55.tar.gz",
    "ref": "3.55",
    "source_folder": "nss",
    "build_configs": [
        {
            "pattern": "aarch64-linux*",
            "patches": [
                "nss-standalone-1.patch"
            ],
            "build_tool": "gyp",
            "library_type": "shared",
            "env_vars": [],
            "options": [
                "-v",
                "-g",
                "--disable-tests",
                "--with-nspr=${INSTALLED_DIR}/include/nspr:${INSTALLED_DIR}/lib",
                "--target=aarch64",
                "-Duse_hw_aes=1",
                "-Duse_arm_neon=1",
                "-Dsign_libs=0"
            ],
            "dependencies": [
                "sqlite3@3.49.0",
                "zlib@v1.3.1",
                "nspr@4.36"
            ],
            "dev_dependencies": [
                "nspr@4.36"
            ]
        },
        {
            "pattern": "x86_64-linux*",
            "patches": [
                "nss-standalone-1.patch"
            ],
            "build_tool": "gyp",
            "library_type": "shared",
            "env_vars": [],
            "options": [
                "-v",
                "-g",
                "--disable-tests",
                "--with-nspr=${INSTALLED_DIR}/include/nspr:${INSTALLED_DIR}/lib",
                "--target=x86_64",
                "-Duse_hw_aes=1",
                "-Duse_arm_neon=1",
                "-Dsign_libs=0"
            ],
            "dependencies": [
                "sqlite3@3.49.0",
                "zlib@v1.3.1",
                "nspr@4.36"
            ],
            "dev_dependencies": [
                "nspr@4.36"
            ]
        }
    ]
}