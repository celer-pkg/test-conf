{
    "url": "https://archive.mozilla.org/pub/security/nss/releases/NSS_3_55_RTM/src/nss-3.55.tar.gz",
    "name": "nss",
    "version": "3.55",
    "source_folder":"nss",
    "build_configs": [
        {
            "pattern": "*linux*",
            "patches": [
                "nss-standalone-1.patch"
            ],
            "build_tool": "gyp",
            "library_type": "shared",
            "env_vars": [
                "CFLAGS=-I${INSTALLED_DIR}/include/nspr",
                "CXXFLAGS=-I${INSTALLED_DIR}/include/nspr",
                "CC=${TOOLCHAIN_PREFIX}-gcc",
                "CCC=${TOOLCHAIN_PREFIX}-g++",
                "BUILD_OPT=1",
                "NSPR_INCLUDE_DIR=${INSTALLED_DIR}/include/nspr",
                "NSPR_LIB_DIR=${INATALLED_DIR}/lib",
                "NSS_ENABLE_WERROR=1",
                "USE_STATIC_RTL=1",
                "OS_TEST=arm"
             ],
            "arguments": [
                "-v", 
                "-g", 
                "--disable-tests",
                "--with-nspr=${INSTALLED_DIR}/include:${INSTALLED_DIR}/include/nspr", 
                "-Dsign_libs=0",
                "--target=armv8-a"
            ],
            "dependencies": [
                "sqlite3@3.49.0",
                "zlib@v1.3.1",
                "nspr@4.36"
            ],
            "dev_dependencies": [
                "nspr@4.36"
            ]
        }
    ]
}