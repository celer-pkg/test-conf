{
    "url": "https://archive.mozilla.org/pub/security/nss/releases/NSS_3_107_RTM/src/nss-3.107.tar.gz",
    "name": "nss",
    "version": "3.107",
    "build_configs": [
        {
            "pattern": "*linux*",
            "build_tool": "freestyle",
            "library_type": "shared",
            "env_vars": [ ],
            "arguments": [
                "CCC=${TOOLCHAIN_PREFIX}-g++",
                "BUILD_OPT=1",
                "NSPR_INCLUDE_DIR=${INSTALLED_DIR}/include",
                "NSPR_LIB_DIR=${INATALLED_DIR}/lib",
                "NSS_USE_SYSTEM_SQLITE=1",
                "NSS_ENABLE_WERROR=0",
                "USE_SYSTEM_ZLIB=1",
                "USE_STATIC_RTL=1",
                "OS_TEST=arm"
            ],
            "dependencies": [
                "sqlite3@version-3.47.0",
                "zlib@v1.3.1"
            ]
        }
    ]
}