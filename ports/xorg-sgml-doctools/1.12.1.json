{
    "url": "https://gitlab.freedesktop.org/xorg/doc/xorg-sgml-doctools.git",
    "ref": "xorg-sgml-doctools-1.12.1",
    "build_configs": [
        {
            "pattern": "*linux*",
            "build_tool": "makefiles",
            "library_type": "shared",
            "env_vars": [
                "NOCONFIGURE=1",
                "ACLOCAL_PATH=${INSTALLED_DIR}/share/aclocal"
            ],
            "options": [
                "--host=${HOST}"
            ],
            "dependencies": [
                "util-macros@1.20.2"
            ],
            "dev_dependencies": []
        }
    ]
}