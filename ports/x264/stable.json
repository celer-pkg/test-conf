{
    "url": "https://gitee.com/chooosky/x264.git",
    "name": "x264",
    "ref": "stable",
    "build_configs": [
        {
            "pattern": "*linux*",
            "build_tool": "makefiles",
            "library_type": "shared",
            "env_vars": [ ],
            "options": [
                "--host=${HOST}",
                "--sysroot=${SYSROOT}",
                "--cross-prefix=${CROSS_PREFIX}",
                "--disable-cli",
                "--enable-pic"
            ],
            "dependencies": [ ],
            "cmake_config": "linux_shared"
        }
    ]
}