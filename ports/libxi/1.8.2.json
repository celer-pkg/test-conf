{
    "url": "https://gitlab.freedesktop.org/xorg/lib/libxi.git",
    "name": "libxi",
    "ref": "libXi-1.8.2",
    "build_configs": [
        {
            "pattern": "*linux*",
            "build_tool": "makefiles",
            "library_type": "shared",
            "env_vars": [
                "NOCONFIGURE=1",
                "xorg_cv_malloc0_returns_null=yes"
            ],
            "options": [
                "--host=${HOST}",
                "--disable-docs"
            ],
            "dependencies": []
        }
    ]
}