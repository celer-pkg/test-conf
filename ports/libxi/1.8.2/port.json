{
    "url": "https://gitlab.freedesktop.org/xorg/lib/libxi.git",
    "ref": "libXi-1.8.2",
    "build_configs": [
        {
            "pattern": "*linux*",
            "build_system": "makefiles",
            "library_type": "shared",
            "env_vars": [
                "xorg_cv_malloc0_returns_null=yes"
            ],
            "options": [
                "--host=${HOST}",
                "--disable-docs"
            ]
        }
    ]
}