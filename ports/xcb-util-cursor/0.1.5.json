{
    "url": "https://www.x.org/releases/individual/xcb/xcb-util-cursor-0.1.5.tar.xz",
    "name": "xcb-util-cursor",
    "ref": "0.1.5",
    "build_configs": [
        {
            "pattern": "*linux*",
            "build_tool": "makefiles",
            "library_type": "shared",
            "env_vars": [
                "NOCONFIGURE=1"
            ],
            "options": [
                "--host=${HOST}"
            ],
            "dependencies": [ 
                "xcb-util-image@0.4.1",
                "xcb-util-renderutil@0.3.10"
            ]
        }
    ]
}