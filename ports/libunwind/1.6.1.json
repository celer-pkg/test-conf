{
    "url": "https://download-mirror.savannah.gnu.org/releases/libunwind/libunwind-1.6.1.tar.gz",
    "name": "libunwind",
    "version": "1.6.1",
    "build_configs": [
        {
            "pattern": "*linux*",
            "build_tool": "makefiles",
            "library_type": "shared",
            "env_vars": [
                "NOCONFIGURE=1",
                "LDFLAGS=-Wl,-rpath-link,${BUILD_DIR}/src/.libs"
            ],
            "arguments": [
                "--host=${HOST}"
            ],
            "dependencies": []
        }
    ]
}