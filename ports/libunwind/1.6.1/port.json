{
    "url": "https://download-mirror.savannah.gnu.org/releases/libunwind/libunwind-1.6.1.tar.gz",
    "ref": "1.6.1",
    "build_configs": [
        {
            "pattern": "*linux*",
            "build_system": "makefiles",
            "library_type": "shared",
            "env_vars": [
                "LDFLAGS=-Wl,-rpath-link,${BUILD_DIR}/src/.libs"
            ],
            "options": [
                "--host=${HOST}"
            ],
            "dependencies": []
        }
    ]
}