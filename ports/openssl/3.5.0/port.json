{
    "url": "https://github.com/openssl/openssl.git",
    "ref": "openssl-3.5.0",
    "build_configs": [
        {
            "pattern": "aarch64-linux*",
            "build_system": "makefiles",
            "env_vars": [ ],
            "options": [
                "--openssldir=ssl",
                "--libdir=lib",
                "linux-aarch64",
                "shared",
                "no-docs",
                "no-demos"
            ]
        },
        {
            "pattern": "x86_64-linux*",
            "build_system": "makefiles",
            "env_vars": [ ],
            "options": [
                "--openssldir=ssl",
                "--libdir=lib",
                "linux-x86_64",
                "shared",
                "no-docs",
                "no-demos"
            ]
        },
        {
            "pattern": "*windows*",
            "build_system": "makefiles",
            "build_tools": [
                "nasm",
                "strawberry-perl"
            ],
            "env_vars": [],
            "options": [
                "--openssldir=ssl",
                "--libdir=lib",
                "VC-WIN64A",
                "shared",
                "no-docs",
                "no-demos"
            ]
        }
    ]
}