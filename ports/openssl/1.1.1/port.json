{
    "url": "https://gh-proxy.com/github.com/openssl/openssl/releases/download/OpenSSL_1_1_1/openssl-1.1.1.tar.gz",
    "ref": "1.1.1",
    "build_configs": [
        {
            "pattern": "aarch64-linux*",
            "build_tool": "makefiles",
            "env_vars": [ ],
            "options": [
                "--libdir=lib",
                "linux-aarch64",
                "shared"
            ],
            "dependencies": [ ]
        },
        {
            "pattern": "x86_64-linux*",
            "build_tool": "makefiles",
            "env_vars": [ ],
            "options": [
                "--libdir=lib",
                "linux-x86_64",
                "shared"
            ],
            "dependencies": [ ]
        },
        {
            "pattern": "*windows*",
            "build_tool": "makefiles",
            "env_vars": [ ],
            "options": [
                "--libdir=lib",
                "linux-x86_64",
                "shared"
            ],
            "dependencies": [ ]
        }
    ]
}