{
    "url": "https://github.com/openssl/openssl.git",
    "ref": "OpenSSL_1_1_1w",
    "build_configs": [
        {
            "pattern": "aarch64-linux*",
            "build_system": "makefiles",
            "env_vars": [ ],
            "options": [
                "--libdir=lib",
                "linux-aarch64",
                "shared"
            ],
            "dependencies": [ ]
        },
        {
            "pattern": "x86_64-linux*",
            "build_system": "makefiles",
            "env_vars": [ ],
            "options": [
                "--libdir=lib",
                "linux-x86_64",
                "shared"
            ],
            "dependencies": [ ]
        },
        {
            "pattern": "*windows*",
            "build_system": "makefiles",
            "env_vars": [ ],
            "options": [
                "--libdir=lib",
                "linux-x86_64",
                "shared"
            ],
            "dependencies": [ ]
        }
    ]
}