{
    "url": "https://gitlab.freedesktop.org/fontconfig/fontconfig.git",
    "ref": "2.13.1",
    "build_configs": [
        {
            "pattern": "*linux*",
            "build_tools": [
                "gettext",
                "autopoint"
            ],
            "patches": [],
            "build_system": "makefiles",
            "library_type": "shared",
            "options": [
                "--host=${HOST}"
            ],
            "dependencies": [
                "freetype@2.13.3",
                "expat@2.6.4"
            ]
        }
    ]
}