{
    "url": "https://archive.mesa3d.org/older-versions/10.x/10.6.0/mesa-10.6.0.tar.xz",
    "name": "mesa",
    "version": "10.6.0",
    "build_configs": [
        {
            "pattern": "*linux*",
            "build_tool": "makefiles",
            "library_type": "shared",
            "env_vars": [
                "NOCONFIGURE=1"
            ],
            "arguments": [
                "--host=${HOST}"
            ],
            "dependencies": [
                "glproto@1.4.14",
                "dri2proto@dri2proto-2.8",
                "libxshmfence@libxshmfence-1.3.3",
                "openssl@3.4.1",
                "expat@2.6.4",
                "libdrm@2.4.124"
            ]
        }
    ]
}