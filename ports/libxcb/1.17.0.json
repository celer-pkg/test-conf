{
    "url": "https://xcb.freedesktop.org/dist/libxcb-1.17.0.tar.xz",
    "name": "libxcb",
    "version": "1.17.0",
    "build_configs": [
        {
            "pattern": "*linux*",
            "build_tool": "makefiles",
            "library_type": "shared",
            "env_vars": [ 
                "NOCONFIGURE=1"
            ],
            "options": [
                "--host=${HOST}"
            ],
            "dependencies": [ 
                "pthread-stubs@0.4",
                "xcb-proto@1.17.0",
                "libXau@libXau-1.0.12"
            ]
        }
    ]
}