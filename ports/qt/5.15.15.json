{
    "url": "https://download.qt.io/official_releases/qt/5.15/5.15.15/single/qt-everywhere-opensource-src-5.15.15.tar.xz",
    "name": "qt",
    "ref": "5.15.15",
    "build_configs": [
        {
            "pattern": "*linux*",
            "patches": [],
            "build_tool": "makefiles",
            "library_type": "shared",
            "env_vars": [],
            "options": [
                "-xplatform linux-aarch64-gnu-g++",
                "-sysroot ${SYSROOT}",
                "-extprefix ${PACKAGE_DIR}",
                "-confirm-license",
                "-opensource",
                "-optimize-size",
                "-static-runtime",
                "-release",
                "-xcb",
                "-fontconfig",
                "-dbus",
                "-ssl",
                "-opengl",
                "-bundled-xcb-xinput",
                "-make-tool make",
                "-nomake tests",
                "-nomake examples",
                "-skip qtdoc",
                "-no-zstd",
                "-no-feature-gssapi",
                "-I${INSTALLED_DIR}/include",
                "-L${INSTALLED_DIR}/lib"
            ],
            "dependencies": [
                "libxcb@1.17.0",
                "libxcb-cursor@0.1.5",
                "xcb-util-wm@0.4.2",
                "mesa@17.3.9",
                "openssl@1.1.1",
                "icu@75.1",
                "libjpeg@2.1.0",
                "libdrm@2.4.124",
                "libxkbcommon@1.6.0",
                "libx11@1.8.11",
                "xcb-util@0.4.1",
                "xcb-util-keysyms@0.4.1",
                "xcb-util-renderutil@0.3.10",
                "xcb-util-xrm@1.0",
                "libxrender@0.9.12",
                "freetype@2.13.3",
                "libsm@1.2.5",
                "pthread-stubs@0.4"
            ],
            "dev_dependencies": [
                "libxcb@1.17.0",
                "libxcb-cursor@0.1.5",
                "xcb-util-wm@0.4.2",
                "openssl@1.1.1",
                "xcb-imdkit@1.0.8",
                "icu@75.1",
                "libjpeg@2.1.0",
                "libdrm@2.4.124",
                "libxkbcommon@1.6.0",
                "libx11@1.8.11",
                "xcb-util@0.4.1",
                "xcb-util-keysyms@0.4.1",
                "xcb-util-renderutil@0.3.10",
                "xcb-util-xrm@1.0",
                "libxrender@0.9.12",
                "libxext@1.3.6"
            ]
        }
    ]
}