                "xcb-imdkit@1.0.8",
                "xcb-util-xrm@1.0",
                "xorg-sgml-doctools@1.12.1",
                "pthread-stubs@0.4",
                "libx11@1.8.11",
                "libxcb-image@0.1.5",
                "xcb-util-keysyms@0.4.1",
                "xcb-util-renderutil@0.3.10",
                "xcb-util@0.4.1",
                "libxdmcp@1.1.5",
