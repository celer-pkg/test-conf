{
    "url": "https://download.qt.io/official_releases/qt/5.15/5.15.16/single/qt-everywhere-opensource-src-5.15.16.tar.xz",
    "ref": "5.15.16",
    "build_configs": [
        {
            "pattern": "*linux*",
            "system_tools": [
                "libclang-dev",
                "ruby"
            ],
            "patches": [],
            "build_tool": "makefiles",
            "library_type": "shared",
            "env_vars": [],
            "options": [
                "-xplatform linux-aarch64-gnu-g++",
                "-sysroot ${SYSROOT}",
                "-extprefix ${PACKAGE_DIR}",
                "-confirm-license",
                "-opensource",
                "-optimize-size",
                "-static-runtime",
                "-release",
                "-xcb",
                "-fontconfig",
                "-dbus",
                "-ssl",
                "-opengl",
                "-bundled-xcb-xinput",
                "-make-tool make",
                "-no-zstd",
                "-nomake tests",
                "-nomake examples",
                "-skip qtdoc",
                "-skip qtwebengine",
                "-I${INSTALLED_DIR}/include",
                "-L${INSTALLED_DIR}/lib"
            ],
            "dependencies": [
                "libxcb@1.17.0",
                "libxcb-cursor@0.1.5",
