{
    "url": "https://gitlab.freedesktop.org/xorg/lib/libxkbfile.git",
    "name": "libxkbfile",
    "ref": "libxkbfile-1.1.3",
    "build_configs": [
        {
            "pattern": "*linux*",
            "build_tool": "makefiles",
            "library_type": "shared",
            "env_vars": [
                "NOCONFIGURE=1",
                "ACLOCAL_PATH=${INSTALLED_DIR}/share/aclocal"
            ],
            "options": [
                "--host=${HOST}"
            ],
            "dependencies": [
                "util-macros@1.20.2",
                "libx11@1.8.11"
            ]
        }
    ]
}