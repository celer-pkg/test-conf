{
    "url": "https://www.x.org/releases/individual/xcb/libpthread-stubs-0.4.tar.gz",
    "name": "pthread-stubs",
    "ref": "0.4",
    "build_configs": [
        {
            "pattern": "*linux*",
            "build_tool": "makefiles",
            "autogen_configure": true,
            "library_type": "shared",
            "env_vars": [
                "NOCONFIGURE=1",
                "ACLOCAL_PATH=${INSTALLED_DIR}/share/aclocal"
             ],
            "options": [
                "--host=${HOST}"
            ],
            "dependencies": [
                "util-macros@1.20.2"
            ]
        }
    ]
}