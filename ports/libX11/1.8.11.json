{
    "url": "https://www.x.org/archive/individual/lib/libX11-1.8.11.tar.xz",
    "name": "libX11",
    "version": "1.8.11",
    "build_configs": [
        {
            "pattern": "*linux*",
            "build_tool": "makefiles",
            "library_type": "shared",
            "env_vars": [
                "ACLOCAL_PATH=${INSTALLED_DIR}/share/aclocal"
            ],
            "options": [
                "--host=${HOST}",
                "--with-keysymdefdir=${INSTALLED_DIR}/include/X11"
            ],
            "dependencies": [
                "util-macros@1.20.2",
                "libxtrans@xtrans-1.5.2",
                "xorgproto@xorgproto-2024.1",
                "libxcb@1.17.0",
                "libXau@libXau-1.0.12"
            ]
        }
    ]
}