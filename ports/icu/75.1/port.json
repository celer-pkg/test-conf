{
    "url": "https://gitee.com/hengke/icu.git",
    "ref": "release-75-1",
    "source_folder": "icu4c/source",
    "build_configs": [
        {
            "pattern": "aarch64-linux*",
            "build_system": "makefiles",
            "library_type": "shared",
            "env_vars": [],
            "options": [
                "--host=${HOST}",
                "--with-cross-build=${BUILD_DIR}/../dev"
            ],
            "dev_dependencies": [
                "icu@75.1"
            ]
        },
        {
            "pattern": "x86_64-linux*",
            "build_system": "makefiles",
            "library_type": "shared",
            "env_vars": [],
            "options": [
                "--host=${HOST}"
            ]
        },
        {
            "pattern": "*windows*",
            "build_system": "makefiles",
            "library_type": "shared",
            "build_tools": [
                "python3"
            ],
            "env_vars": [],
            "options": [
                "--host=${HOST}"
            ]
        }
    ]
}