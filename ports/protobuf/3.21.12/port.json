{
    "url": "https://github.com/protocolbuffers/protobuf.git",
    "ref": "v3.21.12",
    "build_configs": [
        {
            "pattern": "*",
            "build_system": "cmake",
            "library_type": "shared",
            "cpp_standard": "c++17",
            "env_vars": [
                "PATH=${BUILD_DIR}/bin"
            ],
            "options": [
                "-DTHREADS_PREFER_PTHREAD_FLAG=TRUE",
                "-Dprotobuf_BUILD_TESTS=OFF"
            ],
            "dependencies": []
        }
    ]
}