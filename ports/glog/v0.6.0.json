{
    "url": "https://gitee.com/optimum/glog.git",
    "name": "glog",
    "version": "v0.6.0",
    "build_configs": [
        {
            "pattern": "*linux*",
            "build_tool": "ninja",
            "library_type": "shared",
            "options": [
                "-DBUILD_TESTING=OFF",
                "-DWITH_GTEST=OFF",
                "-DBUILD_EXAMPLES=OFF"
            ],
            "dependencies": [
                "gflags@v2.2.2"
            ]
        }
    ]
}